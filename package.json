{
  "name": "construction-field-app",
  "main": "index.js",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "npx expo export --platform web",
    "web:serve": "node scripts/serve-web.js",
    "web:full": "npm run web:build && npm run web:serve",
    "lint": "expo lint",
    "test": "jest --watch --no-coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:prod": "eas build --platform ios --profile production",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:all:prod": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^3.0.1",
    "@react-native-community/netinfo": "^11.5.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.0.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@sentry/react-native": "^8.1.0",
    "@shopify/flash-list": "^2.2.2",
    "date-fns": "^3.6.0",
    "expo": "~54.0.33",
    "expo-camera": "~15.0.12",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.1",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~15.0.1",
    "expo-location": "^19.0.8",
    "expo-media-library": "^18.2.1",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.1",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^10.0.0",
    "immer": "^10.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.52.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "^9.2.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.9.0",
    "@types/react": "~19.1.0",
    "@types/react-native": "~0.73.0",
    "@types/react-redux": "^7.1.27",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "react-test-renderer": "^18.2.0",
    "serve": "^14.2.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
